alias cp='cp -i'
alias mv='mv -i'
alias ls='ls -h --color=auto'
alias lh='ls -lh'
alias chx='chmod +x'
alias vim='nvim'
alias keys="setxkbmap -option caps:ctrl_modifier; xcape -e 'Caps_Lock=Escape'"
alias nrepl="clj -M:nrepl -m nrepl.cmdline --middleware '[cider.nrepl/cider-middleware]' --interactive"
alias trash="rm -rf ~/.local/share/vifm/Trash/*"
alias dfh='df . -h'
alias pless='ps aux | less'
